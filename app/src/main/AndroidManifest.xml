<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.evote">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_foreground"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_foreground"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".About.AboutActivity"></activity>
        <activity android:name=".hasilvote.VoteActivity" />
        <activity android:name=".profile.ProfileActivity" />
        <activity android:name=".detail.DetailActivity" />
        <activity android:name=".donevote.DoneVoteActivity" />
        <activity android:name=".home.HomeActivity" />
        <activity android:name=".sigin.LoginActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>